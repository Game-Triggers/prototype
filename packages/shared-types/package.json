{
  "name": "@gametriggers/shared-types",
  "version": "0.1.0",
  "description": "Shared typescript types and interfaces for eureka platform",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "check-types": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "exports": {
